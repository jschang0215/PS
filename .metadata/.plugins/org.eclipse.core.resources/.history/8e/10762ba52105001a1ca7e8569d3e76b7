#include <iostream>
#include <map>
#include <vector>
#include <string>
using namespace std;

int main() {
	vector<string> first;
	first = {"python", "c++", "c#", "c"};
	vector<string> second;
	second = {"c", "python", "python", "c++"};
	map<string, int> dic;

	for(int i=0; i<first.size(); i++) {
		dic[first[i]] = 0;
		dic[second[i]] = 0;
	}

	for(int i=0; i<first.size(); i++) {
		dic[first[i]]++;
		dic[second[i]]++;
	}

	int ans = 0;
//	map<string, int>::iterator it;
	for(auto it=dic.begin(); it!=dic.end(); it++) {
		ans = max(ans, it->second);
	}

	cout << ans << endl;
	return 0;
}
